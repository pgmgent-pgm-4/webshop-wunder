    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-recommended": "4.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "3.0.0",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "sass": "^1.34.0",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": "14.15.X"
  }
}
